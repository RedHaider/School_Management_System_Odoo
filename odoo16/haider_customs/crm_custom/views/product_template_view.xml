<?xml version="1.0" encoding="utf-8"?>   
<odoo>

    <record id="techtrioz_product_template_form_view_inherit" model="ir.ui.view">
        <field name="name">techtrioz.product.template.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='product_tag_ids']" position='after'>
                <field name="product_brand"/>
                <field name="dealer_name"/>

            </xpath>
        </field>
    </record>

</odoo>